{
  "name": "release-workitemtree",
  "version": "1.0.3",
  "description": "Azure DevOps web extension: Release Work Item Tree",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mikebeem/azure-pipelines-release-workitemtree.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev",
    "postbuild": "npm run package-extension -- --rev-version",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json src/**/*.json",
    "publish-extension": "tfx extension publish --manifest-globs azure-devops-extension.json src/**/*.json --overrides-file configs/release.json --token $TOKEN --rev-version",
    "publish-extension:dev": "tfx extension publish --manifest-globs azure-devops-extension.json src/**/*.json --overrides-file configs/dev.json --token $TOKEN --rev-version"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@types/react-csv": "^1.1.1",
    "antd": "^4.16.3",
    "azure-devops-extension-api": "~1.157.0",
    "azure-devops-extension-sdk": "~2.0.11",
    "export-to-csv": "^0.2.1",
    "rc-mentions": "^1.6.1",
    "react": "^16.13.0",
    "react-app-polyfill": "^1.0.6",
    "react-csv": "^2.0.1",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "azure-devops-ui": "^2.167.6",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "dayjs": "^1.10.5",
    "file-loader": "~2.0.0",
    "less": "^3.13.1",
    "less-loader": "^5.0.0",
    "node-sass": "^4.14.1",

    "react-data-export": "^0.6.0",
    "rimraf": "^2.7.1",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.6.4",
    "ts-loader": "~5.2.2",
    "typescript": "^3.9.10",
    "webpack": "^5.39.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "browserslist": {
    "production": [
      "ie 11",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
