{
    "manifestVersion": 1,
    "id": "#{EXTENSIONID}#",
    "publisher": "#{PUBLISHER}#",
    "version": "#{VERSION}#",
    "name": "#{EXTENSIONNAME}#",
    "description": "Show changed work items in a tree view.",
    "scopes": [
        "vso.project",
        "vso.build",
        "vso.release",
        "vso.work"
      ],
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "Work Items",
        "Work Item",
        "Git",
        "Release",
        "Commit",
        "Visualization"
    ],
    "icons": {
        "default": "logo.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "links": {
        "support": {
          "uri": "https://github.com/Mikebeem/azure-pipelines-release-workitemtree/issues"
        }
      },
    "repository": {
        "type": "git",
        "uri": "https://github.com/Mikebeem/azure-pipelines-release-workitemtree"
    },
    "screenshots": [
        {
          "path": "static/screenshot-workitem-tree.png"
        }
      ],
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}
